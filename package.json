{
  "name": "zfs-assist",
  "displayName": "zfs-assist",
  "description": "zfs-assist",
  "publisher": "nined",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.49.0"
  },
  "repository": "https://github.com/nined9/zfs-assist",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:zfs-assist.entryComponent"
  ],
  "icon": "images/icon.png",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "zfs-assist.entryComponent",
        "title": "Extends Component"
      }
    ],
    "keybindings": [
      {
        "command": "zfs-assist.entryComponent",
        "key": "ctrl+shift+f9",
        "mac": "cmd+shift+f9"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runText.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.49.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.11.7",
    "eslint": "^7.9.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  }
}
